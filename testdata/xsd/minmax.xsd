<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Temperature">
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="-50"/>
                <xs:maxInclusive value="100"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
</xs:schema>