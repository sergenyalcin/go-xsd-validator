<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Category">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Name" type="xs:string"/>
                <xs:element name="SubCategory" type="Category" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>